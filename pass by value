#include<stdio.h>
void fun(int,int);

void main(){
    
    int x=1;
    int y=2;
     fun(x,y);
    printf("inside calling function");
    printf("x= %d y= %d", x,y);
    
}

void fun(int a,int b ){
    a=2;
    b=1;
    printf("inside called function");
    printf("a =%d b=%d",a,b);
    printf("\n");
}
