#include<stdio.h>
char fun();// use data type's name (in this case char not void)

void main(){
     char cv;
     cv=fun();
     printf("The character is %c ",cv);
}


char fun(){//// use data type's name (in this case char not void)
    char c;
    printf("Enter a character: ");
    scanf("%c",&c);
    return c;
    
}

